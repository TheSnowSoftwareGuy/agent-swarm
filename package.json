{
  "name": "agent-swarm",
  "version": "1.0.0",
  "description": "Multi-agent system for autonomous task execution",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-esm src/index.ts",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \\"src/**/*.ts\\"",
    "prepare": "husky install"
  },
  "keywords": [
    "ai",
    "agents",
    "multi-agent",
    "autonomous",
    "typescript"
  ],
  "author": "jarrodwatts",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "openai": "^4.73.1",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "fastify": "^4.25.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}